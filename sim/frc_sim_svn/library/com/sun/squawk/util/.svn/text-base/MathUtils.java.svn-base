package com.sun.squawk.util;

/**
 *
 * @author wolf
 */
public class MathUtils {

	public static double log(double a) {
		return Math.log(a);
	}

	public static double pow(double x, double y) {
		return Math.pow(x, y);
	}

	public static double atan2(double y, double x) {
		return Math.atan2(y, x);
	}
}
